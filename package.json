{
  "name": "ant-design-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "fetch:blocks": "node ./scripts/fetch-blocks.js",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "lint-staged:ts": "tslint",
    "lint:fix": "eslint --fix --ext .js src tests && npm run lint:style && npm run tslint:fix",
    "lint:js": "eslint --ext .js src tests",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:ts": "tslint -p . -c tslint.yml",
    "prettier": "prettier -c --write '**/*'",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tslint:fix": "tslint --fix \"src/**/*.ts*\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{ts,tsx}": "npm run lint-staged:ts"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/pro-layout": "^4.5.14",
    "@antv/data-set": "^0.10.2",
    "@types/lodash.debounce": "^4.0.6",
    "antd": "^3.22.0",
    "axios": "^0.19.0",
    "bizcharts": "^3.5.5",
    "bizcharts-plugin-slider": "^2.1.1",
    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "hash.js": "^1.1.7",
    "lodash": "^4.17.15",
    "lodash-decorators": "^6.0.1",
    "lodash.debounce": "^4.0.8",
    "memoize-one": "^5.0.5",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^3.0.0",
    "prop-types": "^15.7.2",
    "qs": "^6.8.0",
    "rc-animate": "^2.9.2",
    "react": "^16.9.0",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.9.0",
    "react-fittext": "^1.0.0",
    "react-media": "^1.9.2",
    "react-media-hook2": "^1.1.2",
    "redux": "^4.0.4",
    "umi": "^2.8.18",
    "umi-plugin-ga": "^1.1.5",
    "umi-plugin-pro-block": "^1.3.4",
    "umi-plugin-react": "^1.9.17",
    "umi-request": "^1.2.2"
  },
  "devDependencies": {
    "@ant-design/colors": "^3.1.0",
    "@types/classnames": "^2.2.9",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.137",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.2",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.9.0",
    "antd-pro-merge-less": "^1.0.0",
    "babel-eslint": "^10.0.2",
    "chalk": "^2.4.2",
    "check-prettier": "^1.0.3",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.1.1",
    "enzyme": "^3.9.0",
    "eslint": "^6.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.14.3",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.4",
    "jest-puppeteer": "^4.3.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.10.1",
    "lint-staged": "^9.2.3",
    "mockjs": "^1.0.1-beta3",
    "node-fetch": "^2.6.0",
    "prettier": "^1.17.1",
    "slash2": "^2.0.0",
    "stylelint": "^10.0.1",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^3.0.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "webpack-theme-color-replacer": "^1.2.17"
  },
  "optionalDependencies": {
    "puppeteer": "^1.17.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
